{
  "name": "wma-matching-algorithm",
  "version": "1.0.0",
  "description": "The WMA - Matching Algorithm is a light-weighted and fast library to match an object against an object array. Define matching keys and match. Make as many matching objects as you want.",
  "author": "Gert-Jan Wille <gertjanwille.w@gmail.com>",
  "main": "lib/wma.js",
  "module": "src/index.js",
  "repository": {
   "type": "git",
   "url": "https://github.com/gert-janwille/WMA"
 },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run clean && npm run serve . & NODE_ENV=development webpack --watch",
    "serve": "ws -d ./ -c -p 3000 -s ./demo/index.html",
    "clean": "rimraf ./lib",
    "production": "npm run clean && NODE_ENV=production webpack -p",
    "test": "jest"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.17.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "jest": "^22.4.2",
    "local-web-server": "^1.2.6",
    "rimraf": "^2.6.2",
    "webpack": "^2.2.0",
    "webpack-config-utils": "^2.3.0"
  },
  "keywords": [
        "matching algorithm",
